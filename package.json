{
  "name": "@jetbrains/stylelint-config",
  "version": "1.3.3",
  "description": "A stylelint config by JetBrains",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JetBrains/stylelint-config-jetbrains.git"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "properties-order.json"
  ],
  "keywords": [
    "stylelint",
    "stylelint-config",
    "jetbrains"
  ],
  "author": "Andrey Skladchikov <andrey.skladchikov@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/JetBrains/stylelint-config-jetbrains/issues"
  },
  "homepage": "https://github.com/JetBrains/stylelint-config-jetbrains#readme",
  "dependencies": {
    "stylelint-config-css-modules": "1.3.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-order": "^0.8.1"
  },
  "devDependencies": {
    "ci-publish": "^1.3.1",
    "commitizen": "^2.10.1",
    "cz-conventional-changelog": "^2.1.0",
    "standard-version": "^4.4.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "scripts": {
    "commit": "git-cz",
    "release:ci": "standard-version && git push --follow-tags origin master && ci-publish"
  }
}
